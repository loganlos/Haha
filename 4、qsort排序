
#include <stdio.h>
#include <stdlib.h>


/* int */
int compare(const void *a, const void *b) {
    return *(int*)a - *(int*)b;
}

/* structure */
int compare(const void *a, const void *b) {
    Facility *p = (Facility *)a, *q = (Facility *)b;
    return q->v - p->v;
}




int main() {
    int arr[] = {3, 1, 4, 2, 5};
    int n = sizeof(arr) / sizeof(arr[0]);
    qsort(arr, n, sizeof(int), compare);
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");
    return 0;
}
