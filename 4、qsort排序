#include <stdio.h>
#include "string.h"
#include <stdlib.h>

/* int、char类型 */
int cmp_int(const void* _a , const void* _b)　　
{
    int* a = (int*)_a;    //强制类型转换
    int* b = (int*)_b;
    return *a - *b;　　
}


/* double类型 */
int cmp_double(const void* _a , const void* _b)　　
{
    double* a = (double*)_a;    //强制类型转换
    double* b = (double*)_b;
    return *a > *b ? 1 : -1;　  //特别注意
}


/* 字符串类型 */
int cmp_string(const void* _a , const void* _b)　
{
    char* a = (char*)_a;　　//强制类型转换
    char* b = (char*)_b;
    return strcmp(a,b);
}


int main()
{
  qsort(函数名a,排序长度len,sizeof(a[0]),cmp_...);
}
